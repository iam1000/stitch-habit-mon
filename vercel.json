{
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "framework": "vite",
    "rewrites": [
        {
            "source": "/((?!api/.*).*)",
            "destination": "/index.html"
        }
    ],
    "functions": {
        "api/**/*.js": {
            "runtime": "nodejs18.x"
        }
    },
    "env": {
        "VITE_API_BASE_URL": "/api"
    }
}